{
  "name": "web3-patches",
  "version": "1.1.0",
  "description": "Collection of patches to fix web3.js@1.0.0-beta.37",
  "keywords": [
    "bug",
    "ethereum",
    "fix",
    "patch",
    "web3"
  ],
  "license": "MIT",
  "author": "Gabriel Montes <gabriel@bloq.com>",
  "files": [
    "patches"
  ],
  "repository": "autonomoussoftware/web3-patches",
  "scripts": {
    "prepublishOnly": "tag-matches",
    "postinstall": "npm run copypatches",
    "copypatches": "run-script-os",
    "copypatches:darwin:linux": "if [ \"$PWD\" != \"$INIT_CWD\" ] ; then mkdir -p \"$INIT_CWD\"/patches && cp -f patches/* \"$INIT_CWD\"/patches ; fi",
    "copypatches:win32": "if not \"%PWD%\" == \"%INIT_CWD%\" (mkdir \"%INIT_CWD%\"\\patches && xcopy /y patches\\* \"%INIT_CWD%\"\\patches)"
  },
  "dependencies": {
    "patch-package": "^5.0.0",
    "websocket-reconnector": "^1.1.1"
  },
  "devDependencies": {
    "check-tag-matches": "^1.0.0",
    "run-script-os": "^1.0.7"
  },
  "peerDependencies": {
    "web3": "1.0.0-beta.37"
  }
}
