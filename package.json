{
  "name": "web3-patches",
  "version": "1.2.4",
  "description": "Collection of patches to fix web3.js@1.0.0-beta.37",
  "keywords": [
    "bug",
    "ethereum",
    "fix",
    "patch",
    "web3"
  ],
  "license": "MIT",
  "author": "Gabriel Montes <gabriel@bloq.com>",
  "files": [
    "patches"
  ],
  "repository": "autonomoussoftware/web3-patches",
  "scripts": {
    "prepublishOnly": "tag-matches",
    "i:darwin:linux:win32": "cd .",
    "install:darwin:linux:win32": "cd .",
    "postinstall": "run-script-os",
    "postinstall:darwin:linux": "mkdir -p \"$INIT_CWD\"/patches && cp -f \"$CWD\"/patch-files/* \"$INIT_CWD\"/patches && ls \"$INIT_CWD\"/patches",
    "postinstall:win32": "mkdir \"%INIT_CWD%\"\\patches && xcopy /y \"%CWD%\"\\patch-files\\* \"%INIT_CWD%\"\\patches && dir \"%INIT_CWD%\"\\patches"
  },
  "dependencies": {
    "patch-package": "^5.0.0",
    "websocket-reconnector": "^1.1.1"
  },
  "devDependencies": {
    "check-tag-matches": "^1.0.0",
    "run-script-os": "arisi/run-script-os#b0903c4"
  },
  "peerDependencies": {
    "web3": "1.0.0-beta.37"
  }
}
